pipeline {
    agent any

    environment {
        APP = 'WebApi ICDataPrep'
		RELEASE = '1.0.0'
		IMAGENAME = "webapi_icdataprep"
		DOCKERIMAGE = ''
    }

    stages {
        stage('Deploy') {
            steps {
                echo "Desplegando imagen ${APP} release ${RELEASE}"
				script {
					    bat "dotnet --version"
				}
			}
        }
    }
}